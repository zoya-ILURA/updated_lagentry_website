{
  "name": "welcome-page",
  "version": "1.0.0",
  "description": "A welcome page built with React and TypeScript",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run sync:videos && npm run sync:slides",
    "prebuild": "npm run sync:videos && npm run sync:slides",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "sync:videos": "cpx \"videos/**/*\" public/videos || true",
    "sync:slides": "cpx \"../stacked-slides-app/dist/**/*\" public/videos/stacked-slides-app || true",
    "server": "cd server && npm install && npm start",
    "server:dev": "cd server && npm install && npm run dev",
    "dev": "concurrently \"npm start\" \"npm run server:dev\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "emailjs-com": "^3.2.0",
    "firebase": "^10.7.1",
    "gsap": "^3.13.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "concurrently": "^8.2.2",
    "cpx": "^1.5.0",
    "typescript": "^4.9.5"
  },
  "proxy": "http://localhost:5001",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
